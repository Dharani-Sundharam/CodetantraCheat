package q2037;
import java.util.*;

public class PlaylistManager {
    public static void main(String[] args) {
        LinkedList<String> playlist = new LinkedList<>();
        Scanner scanner = new Scanner(System.in);

        while (true) {
            int choice = scanner.nextInt();
            scanner.nextLine(); // consume the newline character

            switch (choice) {
                case 1:
                    // Add song to the beginning
                    String song1 = scanner.nextLine();
playlist.addFirst(song1);
                    break;
                case 2:
                    // Add song using add() (adds to the end)
                    String song2 = scanner.nextLine();
                    playlist.add(song2);
                    break;
                case 3:
                    // Add song to the end explicitly
                    String song3 = scanner.nextLine();
                    playlist.addLast(song3);
                    break;
                case 4:
                    // Exit and print playlist
                    System.out.println(playlist);
                    return;
				default:

System.out.println("Invalid choice. Please try again.");
            }
        }
    }
}




